<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Parcial_jamir_balcona" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/naranja</item>
        <item name="colorPrimaryVariant">@color/naranja</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/naranja</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Base.Theme.Parcial_jamir_balcona.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Base.Theme.Parcial_jamir_balcona.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Base.Theme.Parcial_jamir_balcona.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>